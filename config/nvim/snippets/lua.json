{
    "require": {
        "prefix": "req",
        "body": ["require(${1:module})"]
    },
    "if": {
        "prefix": "if",
        "body": ["if ${1:true} then", "\t$0", "end"]
    },
    "for": {
        "prefix": "for",
        "body": ["for ${1:i}=${2:1},${3:10} do", "\t$0", "end"]
    },
    "foreach": {
        "prefix": "fore",
        "body": ["for i, ${1:x} in pairs(${2:table}) do", "\t$0", "end"]
    },
    "function": {
        "prefix": "fu",
        "body": ["function ${1:name}($2)", "\t${0}", "end"]
    }
}
