{
    "links": {
        "prefix": "link",
        "body": ["[${1:text}](${2:url}) ${0}"]
    },
    "url": {
        "prefix":  "url",
        "body": ["<${1}> ${0}"]
    },
    "images": {
        "prefix": "img",
        "body": ["![${1:alt text}](${2:path}) ${0}"]
    },
    "code": {
        "prefix": "code",
        "body": ["```${1:language}", "$0", "```"]
    }
}
